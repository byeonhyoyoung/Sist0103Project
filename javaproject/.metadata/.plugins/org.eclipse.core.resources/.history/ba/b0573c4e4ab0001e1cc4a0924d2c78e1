package day0111;

/*
 *  이름   MBTI  나이
 * ================
 * 이효리  ENTP  26세
 * 이상순  ISFJ  33세
 * 이영자  ENFJ  28세
 */

class MyInfo{
	
	private String name;
	private String mbti;
	private int age;
	
	public MyInfo(String name,String mbti,int age) {
		this.name=name;
		this.mbti=mbti;
		this.age=age;
	}
	public static void showTitle()
	{
		System.out.println("이름\tMBTI\t나이");
		System.out.println("------------------------");
	}
	
	public void showInfo()
	{
		System.out.println(name+"\t"+mbti+"\t"+age+"세");
	}
}


//////////////////////////
public class QuizArrObTest_12 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		MyInfo []myInfos=new MyInfo[3];
		
		MyInfo [] stu= {
				new MyInfo1("이민호", "010-111-2222", 88),
				new MyInfo2("이영자", "010-333-4444", 77),
				new MyInfo3("김민정", "010-666-7777", 98)
		};
		

	}

}
