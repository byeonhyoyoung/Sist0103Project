package day0110;

public class ArrJungbokSort_06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] lotto=new int[6];
		
		//임의의 수(랜덤수)를 로또변수에 발생시켜서 넣어준다
		for(int i=0;i<lotto.length;i++)
		{
			//1~45까지의 랜덤수 발생
			lotto[i]=(int)(Math.random()*45)+1; //실수를 정수로 형변환
			
		}
	}

}
