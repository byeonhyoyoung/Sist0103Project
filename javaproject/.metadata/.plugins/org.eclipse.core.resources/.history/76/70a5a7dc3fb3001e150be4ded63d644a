package day0115;

abstract class Game{
	abstract public void play();
	abstract public void stop();
}
/////////////////////////
class Kim extends Game{

	@Override
	public void play() {
		// TODO Auto-generated method stub
		System.out.println("kim게임시작");	
	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		System.out.println("kim게임중단");	
	}
	
}
///////////////////
class Lee extends Game{

	@Override
	public void play() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void stop() {
		// TODO Auto-generated method stub
		
	}
	
}
