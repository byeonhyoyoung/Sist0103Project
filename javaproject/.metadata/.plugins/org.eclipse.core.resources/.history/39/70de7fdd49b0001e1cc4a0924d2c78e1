package day0111;

/*
 *  이름   MBTI  나이
 * ================
 * 이효리  ENTP  26세
 * 이상순  ISFJ  33세
 * 이영자  ENFJ  28세
 */

class MyInfo{
	
	private String name;
	private String mbti;
	private int age;
	
	public MyInfo(String name,String mbti,int age) {
		this.name=name;
		this.mbti=mbti;
		this.age=age;
	}
	public void writeData()
	{
		System.out.println("이름: "+name);
		System.out.println("MBTI: "+mbti);
		System.out.println("나이: "+age);
	}
}


