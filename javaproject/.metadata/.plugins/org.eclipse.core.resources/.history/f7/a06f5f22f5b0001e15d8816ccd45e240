package day0112;

class Emart{
	
	private String sangpum;
	private int su;
	
	public Emart() {
		//sangpum="딸기";
		//su=10;
		this("딸기", 10); //<-이와같이 초기화 호출해서 넣어주는 방법도 있다
	}
	
	public Emart(String sangpum,int su) {
		this.sangpum=sangpum;
		this.su=su;
	}
}
