package day0112;

class Food{
	
	private String foodName;
	private int fPrice;
	
	static int no;
	
	//명시적생성자(2개)
	
	public Food(String n, int p) {
		foodName = n;
		fPrice = p;
	}
	
	public void setMember(String sangpum,int price,String sangColor)
	{
		this.setSangpum(sangpum);
		setPrice(price);
		this.setSangColor(sangColor);
	}
	
	//출력문
}

